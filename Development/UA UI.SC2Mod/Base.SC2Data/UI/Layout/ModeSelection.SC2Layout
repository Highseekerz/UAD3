      <!-- Mode Label Frame -->
      <Frame type="Frame" name="ModeLabel">
         <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
         <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
         <Height val="100" />
         <Width val="350" />
         <RenderPriority val="650" />
         <Visible val="False" />
         <Frame type="Image" name="Background">
            <Anchor relative="$parent" />
            <Alpha val="125" />
            <Texture val="Assets\Textures\ui_ingame_leaderboard_frame.dds" />
            <TextureType val="Border" layer="0" />
            <Color val="0,255,148" />
         </Frame>
         <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
            <Color val="255,255,255" />
            <Style val="BattlenetTopNav" />
            <Text val="Mode Name" />
            <TooltipAnchorPosition val="BottomCenter" />
            <Tooltip val="Mode" />
            <TooltipFrame val="UADStandardTooltip" />
            <AcceptsMouseTooltip val="True" />
            <Handle val="GameModeLabel"/>
            <StateGroup name="ModeLabelTooltip">
               <DefaultState val="" />
               <State name="Recruit">
                  <When type="AnimationState" ModeTooltip="1" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_RecruitDesc" />
               </State>
               <State name="Normal">
                  <When type="AnimationState" ModeTooltip="2" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_NormalDesc" />
               </State>
               <State name="Hard">
                  <When type="AnimationState" ModeTooltip="3" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_HardDesc" />
               </State>
               <State name="Insane">
                  <When type="AnimationState" ModeTooltip="4" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_InsaneDesc" />
               </State>
               <State name="Nightmare">
                  <When type="AnimationState" ModeTooltip="5" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_NightmareDesc" />
               </State>
               <State name="Survival">
                  <When type="AnimationState" ModeTooltip="6" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_ClassicSurvivalDesc" />
               </State>
               <State name="MissionSurvival">
                  <When type="AnimationState" ModeTooltip="7" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_MissionSurvivalDesc" />
               </State>
               <State name="BossRush">
                  <When type="AnimationState" ModeTooltip="8" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_BossRushDesc" />
               </State>
               <State name="PMC">
                  <When type="AnimationState" ModeTooltip="9" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_PMCDesc" />
               </State>
               <State name="SandBox">
                  <When type="AnimationState" ModeTooltip="10" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_SandBoxDesc" />
               </State>
               <State name="Deathmatch">
                  <When type="AnimationState" ModeTooltip="11" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_DeathmatchDesc" />
               </State>
               <State name="WeeklyMutation">
                  <When type="AnimationState" ModeTooltip="12" />
                  <Action type="SetProperty" frame="$this" Tooltip="@DOCSTR_WeeklyMutationDesc" />
               </State>
            </StateGroup>
            <StateGroup name="AnchorState">
               <DefaultState val="NoModifiers" />
               <State name="HasModifiers">
                  <When type="Property" frame="$parent/Icon0" Visible="True" />
                  <Action type="SetAnchor" frame="$this" side="Top" relative="$parent" pos="Min" offset="-10"/>
               </State>
               <State name="NoModifiers" />
            </StateGroup>
         </Frame>
         <Frame type="Image" name="FirstWinBonusFrame">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-4" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="-4" />
            <Width val="32" />
            <Height val="32" />
            <Texture val="Assets/Accessory Textures/firstwin_Off.dds" />
            <AcceptsMouse val="True" /> 
            <RenderPriority val="850" />
            <Tooltip val="First Win XP Bonus Not Available" />
            <TooltipFrame val="UADStandardTooltip" />
            <Frame type="Image" name="FirstWinBonusActiveImage">
               <Anchor relative="$parent" />
               <Texture val="Assets/Accessory Textures/firstwin_on.dds" />
               <Visible val="False" />
               <Alpha val="0" />
            </Frame>
            <Animation name="FirstWinActive">
               <Event event="FirstWinEnabled" action="DirectionForward,Play" />
               <Event event="FirstWinDisabled" action="DirectionReverse,Play" />
               <Controller type="Fade" end="Pause" frame="FirstWinBonusActiveImage">
                  <Key type="Curve" time="0" value="0" inout="Linear" />
                  <Key type="Curve" time=".4" value="255" inout="Slow,Linear" />
               </Controller>
               <Controller type="Visibility" end="Pause" frame="FirstWinBonusActiveImage">
                  <Key type="Bool" time="0" value="false" />
                  <Key type="Bool" time="0.01" value="true" />
                  <Key type="Bool" time="0.4" value="true" />
               </Controller>
            </Animation>
            <StateGroup name="TextController">
               <DefaultState val="NotActive" />
               <State name="Active">
                  <When type="Property" frame="FirstWinBonusActiveImage" Visible="True" />
                  <Action type="SetProperty" frame="$this" Tooltip="First Win XP Bonus Available" />
               </State>
               <State name="NotActive" />
            </StateGroup>
         </Frame>
         <!-- Modifier Icons -->
         <Frame type="Frame" name="Icon0" template="StandardUADTemplates/ModifierIconTemplate">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="-4" />
