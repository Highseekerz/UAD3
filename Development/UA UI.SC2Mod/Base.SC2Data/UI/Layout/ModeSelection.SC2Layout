         </Frame>
         <Frame type="Label" name="MilliLabel" template="RandomText/BaloneyNumbersMillisecondsTemplate">
            <Anchor relative="$parent" />
            <Alpha val="0" />
         </Frame>
         <Animation name="DisplayTimer">
            <Event event="Display" action="Play" />
            <Controller type="Visibility" end="Pause">
               <Key type="Bool" time="1.10" value="True" />
            </Controller>
         </Animation>
         <Animation name="DestroyTimer">
            <Event event="Destroy" action="Play" />
            <Controller type="Visibility" end="Pause">
               <Key type="Bool" time="0" value="False" />
            </Controller>
         </Animation>
         <StateGroup name="TimerLabel">
            <DefaultState val="" />
            <State name="Hidden">
               <When type="Property" frame="$MapPanel" Visible="False" />
               <Action type="SetProperty" Visible="False" />
            </State>
         </StateGroup>
      </Frame>
