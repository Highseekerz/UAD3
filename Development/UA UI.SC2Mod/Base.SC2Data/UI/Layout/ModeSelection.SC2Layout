         </Frame>   
         <Animation name="FadeIn">
            <Event event="OnShown" action="Reset,Play" />
            <Controller name="FadeIn" type="Fade" end="Pause">
               <Key type="Curve" time="0" value="0" in="Auto" />
               <Key type="Curve" time="1.0" value="0" in="Auto" />
               <Key type="Curve" time="1.75" value="255" out="Auto" />
            </Controller>
            <Controller type="Anchor" end="Pause" side="Left" relative="true">
               <Key type="Curve" time="0" value="40" out="Linear" />
               <Key type="Curve" time="1.0" value="40" out="Linear" />
               <Key type="Curve" time="1.75" value="0" out="Linear" />
            </Controller>
         </Animation>
         <StateGroup name="ModeLabel">
            <DefaultState val="" />
            <State name="Hidden">
               <When type="Property" frame="$MOSSelection" Visible="False" />
               <Action type="SetProperty" Visible="False" />
            </State>
            <State name="Visible">
               <When type="Property" frame="$MOSSelection" Visible="True" />
               <Action type="SetProperty" Visible="True" />
            </State>
         </StateGroup>
      </Frame>
      <!-- Mode Label Frame -->
      <Frame type="Frame" name="ModeTimer">
         <Anchor side="Top" relative="$parent" pos="Mid" offset="1000" />
         <Anchor side="Right" relative="$parent" pos="Mid" offset="0" />
         <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
         <Anchor side="Left" relative="$parent" pos="Mid" offset="1225" />
         <Height val="100" />
         <Width val="150" />
         <RenderPriority val="650" />
         <Visible val="False" />
         <Handle val="ModeTimer" />
         <UseAlternateTime val="True" />
         <Frame type="Label" name="Label">
            <Anchor relative="$parent" />
            <Text val="{$this/FormattedText/@Text}" />
            <Frame type="TextFormatFrame" name="FormattedText">
               <Anchor relative="$parent" />
               <FormatText val="%0%:%1%" />
               <Replacement index="0" val="{$parent/$parent/SecLabel/@Text}" />
               <Replacement index="1" val="{$parent/$parent/MilliLabel/@Text}" />
