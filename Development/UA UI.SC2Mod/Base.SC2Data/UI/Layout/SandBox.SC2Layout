            <LayerAlpha val="20" layer="1" />
            <LayerColor val="0,255,148" layer="1" />
         </Frame>
         <Frame type="Frame" name="TabButtonsContainer">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
            <Height val="85" />
            <Frame type="Button" name="Button00" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
               <Anchor side="Top" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Left" relative="$parent" pos="Min" offset="20" />
               <Color val="0,0,0" />
               <Height val="50" />
               <Width val="125" />
               <Toggleable val="True" />
               <Frame type="Label" name="Label" template="StandardGreyboxTemplates/GenericButtonLabelTemplate">
                  <Text val="Items" />
                  <Color val="255,255,255" />
                  <Style val="BattlenetTopNavSmall" />
               </Frame>
            </Frame>
            <Frame type="Button" name="Button01" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
               <Anchor side="Top" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Left" relative="$parent/Button00" pos="Max" offset="5" />
               <Color val="0,0,0" />
               <Height val="50" />
               <Width val="125" />
               <Toggleable val="True" />
               <Frame type="Label" name="Label" template="StandardGreyboxTemplates/GenericButtonLabelTemplate">
                  <Text val="Units" />
                  <Color val="255,255,255" />
                  <Style val="BattlenetTopNavSmall" />
               </Frame>
            </Frame>
            <Frame type="Button" name="Button02" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
               <Anchor side="Top" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Left" relative="$parent/Button01" pos="Max" offset="5" />
               <Color val="0,0,0" />
               <Height val="50" />
               <Width val="125" />
               <Toggleable val="True" />
               <Frame type="Label" name="Label" template="StandardGreyboxTemplates/GenericButtonLabelTemplate">
                  <Text val="Enemies" />
                  <Color val="255,255,255" />
                  <Style val="BattlenetTopNavSmall" />
               </Frame>
            </Frame>
            <Frame type="Button" name="Button03" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
               <Anchor side="Top" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0" />
               <Anchor side="Left" relative="$parent/Button02" pos="Max" offset="50" />
               <Color val="0,0,0" />
               <Height val="50" />
               <Width val="125" />
               <Toggleable val="True" />
               <Visible val="False" />
               <Alpha val="0" />
            </Frame>
         </Frame>
         <StateGroup name="SelectedState">
            <DefaultState val="LT3" />
            <State name="LT3">
               <When type="Property" frame="$this" operator="LessThan" SelectedIndex="3" />
               <Action type="SetProperty" frame="$SBOptionsIcon" Visible="True" />
               <Action type="SetProperty" frame="$SBOptionsButton" Visible="True" />
            </State>
            <State name="EQ3">
               <When type="Property" frame="$this" SelectedIndex="3" />
               <Action type="SetProperty" frame="$SBOptionsIcon" Visible="False" />
               <Action type="SetProperty" frame="$SBOptionsButton" Visible="False" />
            </State>
         </StateGroup>
         <StateGroup name="ButtonState">
            <DefaultState val="Item" />
            <State name="Item">
               <When type="Property" frame="$this" SelectedIndex="0" />
               <Action type="SetProperty" frame="$SBCursorButton" Enabled="True" />
               <Action type="SetProperty" frame="$SBCursorButton" Visible="True" />
               <Action type="SetProperty" frame="$SBSpawnButton" Enabled="False" />
               <Action type="SetProperty" frame="$SBSpawnButton" Visible="False" />
               <Action type="SetProperty" frame="$SBSpawnGroupButton" Enabled="False" />
               <Action type="SetProperty" frame="$SBSpawnGroupButton" Visible="False" />
               <Action type="SetProperty" frame="$InfoText" Text="Hold 'Alt' while clicking game world to place item" />
            </State>
            <State name="Unit">
               <When type="Property" frame="$this" SelectedIndex="1" />
               <Action type="SetProperty" frame="$SBCursorButton" Enabled="True" />
               <Action type="SetProperty" frame="$SBCursorButton" Visible="True" />
               <Action type="SetProperty" frame="$SBSpawnButton" Enabled="False" />
               <Action type="SetProperty" frame="$SBSpawnButton" Visible="False" />
               <Action type="SetProperty" frame="$SBSpawnGroupButton" Enabled="False" />
               <Action type="SetProperty" frame="$SBSpawnGroupButton" Visible="False" />
               <Action type="SetProperty" frame="$InfoText" Text="Hold 'Alt' while clicking game world to place unit" />
            </State>
            <State name="Enemy">
               <When type="Property" frame="$this" SelectedIndex="2" />
               <Action type="SetProperty" frame="$SBCursorButton" Enabled="False" />
               <Action type="SetProperty" frame="$SBCursorButton" Visible="False" />
               <Action type="SetProperty" frame="$InfoText" Visible="False" />
            </State>
            <State name="Options">
               <When type="Property" frame="$this" SelectedIndex="3" />
               <Action type="SetProperty" frame="$InfoText" Visible="False" />
               <Action type="SetProperty" frame="$SBSpawnButton" Visible="False" />
               <Action type="SetProperty" frame="$SBSpawnGroupButton" Visible="False" />
            </State>
         </StateGroup>
         <Animation name="DisplayAnim">
            <Event event="Display" action="Reset,Play" />
            <Controller type="Visibility" end="Pause">
               <Key type="Bool" time="0" value="True" />
            </Controller>
         </Animation>
         <Frame type="Frame" name="PanelsContainer">
            <Anchor side="Top" relative="$parent/TabButtonsContainer" pos="Max" offset="0" />
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
            <Frame type="Label" name="Info">
               <Anchor side="Top" relative="$parent/CloseButton" pos="Min" offset="-40" />
               <Anchor side="Left" relative="$parent" pos="Min" offset="25" />
               <Height val="25" />
               <Width val="425" />
               <Style val="GameButtonChargeSmall" />
               <Text val="" />
               <Handle val="InfoText" />
            </Frame>
            <Frame type="Button" name="CloseButton" template="StandardUADTemplates/UADAlternateButtonTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-90" />
               <Anchor side="Left" relative="$parent" pos="Min" offset="10" />
               <Width val="175" />
               <Frame type="Label" name="Label">
                  <Anchor side="Left" relative="$parent" pos="Min" offset="5" />
                  <Text val="Close" />
               </Frame>
               <Animation name="ClickAnim">
                  <Event event="OnClick" action="Reset,Play" />
                  <Controller type="Visibility" frame="$SandBoxPanel" end="Pause">
                     <Key type="Bool" time="0" value="False" />
                  </Controller>
                  <Controller type="Event" end="Stop" frame="$SBSpawnMenuButton">
                     <Key type="Event" time="0" event="Display" />
                  </Controller>
               </Animation>
            </Frame>
            <Frame type="Button" name="CursorButton" template="StandardUADTemplates/UADAlternateButtonTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-90" />
               <Anchor side="Left" relative="$parent/CloseButton" pos="Min" offset="230" />
               <Width val="175" />
               <Handle val="SBCursorButton" />
               <Enabled val="True" />
               <Visible val="True" />
               <Shortcut val="F3" />
               <AcceptsMouseTooltip val="True" />
               <Tooltip val="Clear cursor" />
               <TooltipFrame val="UADStandardTooltip" />
               <Frame type="Label" name="Label">
                  <Anchor side="Left" relative="$parent" pos="Min" offset="5" />
                  <Text val="Reset [F3]" />
               </Frame>
            </Frame>
            <Frame type="Button" name="SpawnButton" template="StandardUADTemplates/UADAlternateButtonTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-90" />
               <Anchor side="Left" relative="$parent/CloseButton" pos="Min" offset="230" />
               <Width val="175" />
               <Handle val="SBSpawnButton" />
               <Enabled val="False" />
               <Visible val="False" />
               <Frame type="Label" name="Label">
                  <Anchor side="Left" relative="$parent" pos="Min" offset="5" />
                  <Text val="Spawn" />
               </Frame>
            </Frame>
            <Frame type="Button" name="SpawnGroupButton" template="StandardUADTemplates/UADAlternateButtonTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-90" />
               <Anchor side="Left" relative="$parent/CloseButton" pos="Min" offset="230" />
               <Width val="175" />
               <Handle val="SBSpawnGroupButton" />
               <Enabled val="False" />
               <Visible val="False" />
               <Frame type="Label" name="Label">
                  <Anchor side="Left" relative="$parent" pos="Min" offset="5" />
                  <Text val="Spawn" />
               </Frame>
            </Frame>
            <Frame type="Button" name="OptionsButton" template="StandardGreyboxTemplates/StandardTextButtonSmallTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-200" />
               <Anchor side="Left" relative="$parent" pos="Min" offset="342" />
               <Height val="52" />
               <Width val="52" />
               <Alpha val="0" />
               <AcceptsMouseTooltip val="True" />
               <Tooltip val="Options" />
               <TooltipFrame val="UADStandardTooltip" />
               <RenderPriority val="600" />
               <Handle val="SBOptionsButton" />
               <Animation name="ClickAnim">
                  <Event event="OnClick" action="Reset,Play" />
                  <Controller type="Property" property="SelectedIndex" frame="$SandBoxPanel" end="Pause">
                     <Key type="Property" time="0" Value="3" />
                  </Controller>
               </Animation>
               <Animation name="Hover">
                  <Event event="OnMouseEnter" action="DirectionForward,Play" />
                  <Event event="OnMouseExit" action="DirectionReverse,Play" />
                  <Controller type="Fade" frame="$parent/OptionsIcon/Icon/IconBorder1" end="Pause">
                     <Key type="Curve" time="0" value="0" inout="Linear" />
                     <Key type="Curve" time=".2" value="255" inout="Slow,Linear" />
                  </Controller>
               </Animation>
            </Frame>
            <Frame type="Frame" name="OptionsIcon" template="SandBox/ImageBorderFrameTemplate">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-200" />
               <Anchor side="Right" relative="$parent" pos="Max" offset="295" />
               <Height val="52" />
               <Width val="52" />
               <Handle val="SBOptionsIcon" />
               <Frame type="Image" name="Icon">
                  <Texture val="Assets/Icon Pack/btn-ability-terran-immortalityprotocol-color2.dds" />
               </Frame>
            </Frame>
            <Frame type="TabControl" name="EnemyButtonSwitch">
               <Anchor side="Top" relative="$parent" pos="Max" offset="-145" />
               <Anchor side="Left" relative="$parent" pos="Min" offset="78" />
               <Width val="110" />
               <Height val="60" />
               <Visible val="False" />
               <SelectedIndex val="0" />
               <TabListItem val="$this/TabButtonsContainer/EnemyListButton" />
               <TabListItem val="$this/TabButtonsContainer/EnemyGroupButton" />
               <Frame type="Frame" name="TabButtonsContainer">
                  <Anchor relative="$parent" />
                  <Handle val="EnemySwitch" />
                  <Frame type="Button" name="EnemyListButton" template="StandardUADTemplates/OptionsButtonTemplate">
                     <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
                     <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
                     <Toggleable val="True" />
                     <Height val="52" />
                     <Width val="52" />
                     <Handle val="SBEnemyListButton" />
                     <Tooltip val="Enemy List" />
                     <Frame type="Image" name="Icon">
                        <Texture val="Assets\Textures\btn-upgrade-zerg-swarmfrenzy.dds" />
                     </Frame>
                  </Frame>
                  <Frame type="Button" name="EnemyGroupButton" template="StandardUADTemplates/OptionsButtonTemplate">
                     <Anchor side="Top" relative="$parent/EnemyListButton" pos="Min" offset="0" />
                     <Anchor side="Left" relative="$parent/EnemyListButton" pos="Min" offset="55" />
                     <Toggleable val="True" />
                     <Height val="52" />
                     <Width val="52" />
                     <Tooltip val="Enemy Spawn Groups" />
                     <Frame type="Image" name="Icon">
                        <Texture val="Assets\Textures\btn-progression-zerg-dehaka-10-primalbossupgrades.dds" />
                     </Frame>
                  </Frame>
               </Frame>
               <StateGroup name="EnemyButtonState">
                  <DefaultState val="Hidden" />
                  <State name="Visible">
                     <When type="Property" frame="$SandBoxPanel" SelectedIndex="2" />
                     <Action type="SetProperty" frame="$this" Visible="True" />
                  </State>
                  <State name="Hidden" />
               </StateGroup>
               <StateGroup name="IndexSwitch">
                  <DefaultState val="EnemyListToggled" />
                  <State name="EnemyListToggled">
                     <When type="Property" frame="$this" SelectedIndex="0" />
                     <Action type="SetProperty" frame="$SBEnemyCount" Enabled="True" />
                     <Action type="SetProperty" frame="$SBEnemyCountLabel" Alpha="255" />
                     <Action type="SetProperty" frame="$SBEnemyList" Visible="True" />
                     <Action type="SetProperty" frame="$SBSpawnButton" Enabled="True" />
                     <Action type="SetProperty" frame="$SBSpawnButton" Visible="True" />
                     <Action type="SetProperty" frame="$SBSpawnGroupButton" Enabled="False" />
                     <Action type="SetProperty" frame="$SBSpawnGroupButton" Visible="False" />
                     <Action type="SetProperty" frame="$SBEnemyGroupList" Visible="False" />      
                  </State>
                  <State name="EnemyGroupToggled">
                     <When type="Property" frame="$this" SelectedIndex="1" />
                     <Action type="SetProperty" frame="$SBEnemyCount" Enabled="False" />
                     <Action type="SetProperty" frame="$SBEnemyCountLabel" Alpha="25" />
                     <Action type="SetProperty" frame="$SBEnemyList" Visible="False" />
                     <Action type="SetProperty" frame="$SBSpawnButton" Enabled="False" />
                     <Action type="SetProperty" frame="$SBSpawnButton" Visible="False" />
                     <Action type="SetProperty" frame="$SBSpawnGroupButton" Enabled="True" />
                     <Action type="SetProperty" frame="$SBSpawnGroupButton" Visible="True" />
                     <Action type="SetProperty" frame="$SBEnemyGroupList" Visible="True" />     
                  </State>
