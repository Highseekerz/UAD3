                <Texture val="Assets\Textures\ui_battlenet_glue_columnframe.dds"/>	
                <TextureType val="Border" layer="0"/> 
                <Color val="0,0,0"/> -->
            <Texture val="Assets\Textures\ui_ingame_leaderboard_frame.dds" />
            <TextureType val="Border" />
            <Color val="0,255,148" />
         </Frame>
