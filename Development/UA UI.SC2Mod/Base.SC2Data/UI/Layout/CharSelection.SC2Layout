      <Frame type="Frame" name="CommandCardType">
         <Anchor side="Bottom" relative="$parent/CharSelectionTabs" pos="Max" offset="-35" />
         <Anchor side="Right" relative="$parent/CharSelectionTabs" pos="Max" offset="-210" />
         <Height val="75" />
         <Width val="220" />
         <Visible val="True" />
         <AcceptsMouse val="True" />
         <Frame type="CheckBox" name="Option" template="StandardTemplates/StandardCheckBoxTemplate">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
            <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
            <Checked val="False" />
            <Tooltip val="@DOCSTR_ClassicCard" />
            <TooltipFrame val="UADStandardTooltip" />
         </Frame>
         <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent/Option" pos="Min" offset="2" />
            <Anchor side="Left" relative="$parent/Option" pos="Min" offset="35" />
            <Height val="30" />
            <Width val="350" />
            <Text val="Enable Classic Card" />
            <Style val="Descript_Left" />
            <Color val="255,255,255" />
         </Frame>
         <StateGroup name="MosSelectionVisible">
            <DefaultState val="Hidden" />
            <State name="Hidden">
               <When type="Property" frame="$MOSSelection" Visible="False" />
               <Action type="SetProperty" frame="$this" Visible="False" />
               <Action type="SetProperty" frame="$this" Enabled="False" />
               <Action type="SetProperty" frame="$this" AcceptsMouse="False" />
            </State>
            <State name="Visible">
               <When type="Property" frame="$MOSSelection" Visible="True" />
               <Action type="SetProperty" frame="$this" Visible="True" />
               <Action type="SetProperty" frame="$this" Enabled="True" />
               <Action type="SetProperty" frame="$this" AcceptsMouse="True" />
            </State>
         </StateGroup>
         <Animation name="FadeIn">
            <Event event="OnShown" action="Reset,Play" />
            <Controller type="Fade" end="Pause">
               <Key type="Curve" time="0" value="0" out="Slow" />
               <Key type="Curve" time="0.5" value="255" in="Slow" />
            </Controller>
         </Animation>
      </Frame>

      <Frame type="Frame" name="MovieContainer">
         <Anchor side="Bottom" relative="$parent" pos="Max" offset="25"/>
         <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
         <Height val="128"/>
         <Width val="128"/>
         <RenderPriority val="1200"/>
         <AcceptsMouse val="False"/>
         <Alpha val="125"/>
