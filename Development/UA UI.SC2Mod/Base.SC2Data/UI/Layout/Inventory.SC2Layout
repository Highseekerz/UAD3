<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
   <Constant name="InvButtonSize" val="76" />
   <Constant name="PackButtonSize" val="44" />
   <Constant name="InvButtonGap" val="1" />
   <Constant name="PackButtonGap" val="2" />

   <Animation name="BackpackLockedTemplate">
      <Event event="Locked" action="Reset,Play" /> 
      <Controller type="Property" property="Enabled" end="Pause">
         <Key type="Property" time="0" value="True" />
         <Key type="Property" time="0.1" value="False" />
      </Controller>
   </Animation>
   <StateGroup name="TooltipStateTemplate">
      <DefaultState val="Off" />
      <State name="On">
         <When type="Property" frame="$this" Enabled="False" />
         <Action type="SetProperty" frame="$this" AcceptsMouseTooltip="True" />
      </State>
      <State name="Off" />
   </StateGroup>
   <StateGroup name="AlphaStateTemplate">
      <DefaultState val="Default" />
      <State name="Disabled">
         <When type="Property" frame="$parent" Enabled="False" />
         <Action type="SetProperty" frame="$this" Alpha="255" />
      </State>
      <State name="Enabled">
         <When type="Property" frame="$parent" Enabled="True" />
         <Action type="SetProperty" frame="$this" Alpha="0" />
      </State>
      <State name="Default" />
   </StateGroup>

   <Frame type="CommandButton" name="UADInventoryButton" template="InventoryPanel/ContainerButtonTemplate">
      <ShowCooldownText val="False"/>
      <Frame type="Image" name="GameButtonBorderImage">
         <RenderPriority val="504" />
      </Frame>
      <Frame type="Image" name="GameButtonDefaultHoverBorder">
         <Anchor relative="$parent/GameButtonBorderImage" />
         <Texture val="@UI/ButtonBorderNormalActive_Terr" />
         <RenderPriority val="505" />
         <Alpha val="0" />
         <Animation name="Hover">
            <Event event="OnMouseEnter" action="DirectionForward,Play" frame="$parent" />
            <Event event="OnMouseExit" action="DirectionReverse,Play" frame="$parent" />
            <Event event="Reset" action="Stop" frame="$parent" />
            <Controller type="Fade" end="Pause">
               <Key type="Curve" time="0" value="0" inout="Linear" />
               <Key type="Curve" time=".2" value="255" inout="Slow,Linear" />
            </Controller>
         </Animation>
      </Frame>
      <Frame type="Label" name="CooldownLabel">
         <Anchor side="Top" relative="$this" pos="Max" offset="0"/>
         <Anchor side="Right" relative="$this" pos="Min" offset="0"/>
         <Anchor side="Bottom" relative="$parent" pos="Max" offset="-8"/>
         <Anchor side="Left" relative="$parent" pos="Max" offset="-66"/>
         <Style val="GameButtonCharge"/>
         <RenderPriority val="520"/>
         <Visible val="False"/>
      </Frame>

      <Frame type="UnitFrame" name="Item">
         <Anchor relative="$parent"/>
      </Frame>

      <Frame type="Image" name="NormalImage">
         <Anchor relative="$parent"/>
         <DescFlags val=""/>
         <StateGroup name="Item">
            <DefaultState val="NoItem" />
            <State name="NoItem">
               <When type="Property" frame="$this" Texture="" index="0" />
            </State>
            <State name="HasItem">
               <When type="Property" frame="$this" operator="NotEqual" Texture="" index="0" />
               <Action type="SetProperty" frame="$parent/Item" UnitTag="{$parent/@ButtonOtherUnit}" />
            </State>
         </StateGroup>
      </Frame>

      <Frame type="TextFormatFrame" name="TFT_Button">
         <FormatText val=""/>
      </Frame>

      <Frame type="TextFormatFrame" name="TFT_UnitTag">
         <FormatText val=""/>
      </Frame>

      <Frame type="TextFormatFrame" name="TFT_Final">
         <FormatText val="%0%&#xA;%1%"/>
         <Replacement index="0" val="{$parent/TFT_Button/@Text}"/>
         <Replacement index="1" val="{$parent/TFT_UnitTag/@Text}"/>
      </Frame>
   </Frame>

