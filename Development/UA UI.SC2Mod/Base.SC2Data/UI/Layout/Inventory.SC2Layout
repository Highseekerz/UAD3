            </StateGroup>
            <Frame type="UnitFrame" name="WeaponID">
               <Anchor relative="$parent" />
               <StateGroup name="WeaponSlot">
                  <DefaultState val="NotM5" />
                  <State name="IsM5">
                     <When type="Property" frame="$this" UnitLink="M5MAR" />
                  </State>
                  <State name="NotM5" />
               </StateGroup>
            </Frame> 
