         <!-- Weapon/Armor Tooltip -->
         <Frame type="Image" name="WeaponIcon">
            <Anchor relative="$parent/Slot14" />
            <LayerCount val="2" />
            <TextureType val="Normal" layer="0" />
            <TextureType val="Normal" layer="1" />
            <Texture val="@UI/ButtonBorderNormal_Terr" layer="0" />
            <Texture val="Buttons\BTNItemW.dds" layer="1" />
            <Visible val="True" />
            <AcceptsMouse val="True" />
            <TooltipAnchorPosition val="BottomRight" />
            <Tooltip val="" />
            <TooltipFrame val="UADStandardTooltip" />
            <UseTooltip val="True" />
            <Handle val="WeaponIconImage" />
            <StateGroup name="WeaponType">
               <DefaultState val="NoTooltipData" /> 
               <State name="NoTooltipData" />
               <State name="Rifleman"> 
                  <When type="Property" frame="$NameLabel" Text="Rifleman" />
