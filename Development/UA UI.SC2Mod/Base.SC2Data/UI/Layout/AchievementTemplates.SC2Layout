         <State name="Flash">
            <Action type="SendEvent" frame="ContainerImage" event="Enabled" />
            <Action type="SendEvent" frame="IconFrameImage" event="Enabled" />
            <Action type="SendEvent" frame="InnerContainerImage" event="Enabled" />
            <Action type="SetProperty" Enabled="True" />
         </State>
         <State name="Unlocked">
            <Action type="SendEvent" frame="ContainerImage" event="Enabled" />
            <Action type="SendEvent" frame="IconFrameImage" event="Enabled" />
            <Action type="SendEvent" frame="InnerContainerImage" event="Enabled" />
            <Action type="SetProperty" frame="PointsLabel" Visible="True" />
            <Action type="SetProperty" frame="ProgressBar" Visible="True" />
            <Action type="SetProperty" Enabled="True" />
         </State>
         <State name="Disabled">
            <Action type="SendEvent" frame="ContainerImage" event="Disabled" />
            <Action type="SendEvent" frame="IconFrameImage" event="Disabled" />
            <Action type="SendEvent" frame="InnerContainerImage" event="Disabled" />
            <Action type="SendEvent" frame="AchievementIconImage" event="Disabled" />
         </State>
         <State name="DisabledWithProgress">
            <Action type="SendEvent" frame="ContainerImage" event="Disabled" />
            <Action type="SendEvent" frame="IconFrameImage" event="Disabled" />
            <Action type="SendEvent" frame="InnerContainerImage" event="Disabled" />
            <Action type="SendEvent" frame="AchievementIconImage" event="Disabled" />
            <Action type="SetProperty" frame="PointsLabel" Visible="True" />
            <Action type="SetProperty" frame="ProgressBar" Visible="True" />
            <Action type="SetProperty" frame="EarnedLabel" Visible="False" />
         </State>
      </StateGroup>
      <StateGroup name="EarnedState">
         <DefaultState val="UnEarned" />
         <State name="Earned">
            <When type="AnimationState" AchievementBadge="Earned" />
            <Action type="SetState" group="AchievementBadgeState" state="Enabled" />
         </State>
         <State name="UnEarned" />
      </StateGroup>
