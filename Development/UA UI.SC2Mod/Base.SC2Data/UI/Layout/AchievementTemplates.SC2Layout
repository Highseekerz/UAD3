         <Frame type="Label" name="CurrentLabel">
            <Anchor relative="$parent" />
            <Alpha val="0" />
         </Frame>
         <Frame type="Label" name="MaxLabel">
            <Anchor relative="$parent" />
            <Alpha val="0" />
         </Frame>
         <Frame type="MathFrame" name="MathFrameA">
            <Anchor relative="$parent" />
            <A val="{$parent/CurrentLabel/@Text}" />
            <B val="{$parent/MaxLabel/@Text}" />
            <Operator val="Divide" />
         </Frame>
         <Frame type="MathFrame" name="MathFrameB">
            <Anchor relative="$parent" />
            <A val="{$parent/MathFrameA/@Result}" />
            <B val="100" />
            <Operator val="Multiply" />
         </Frame>
         <Frame type="TextFormatFrame" name="FormattedText">
            <Anchor relative="$parent" />
            <FormatText val="Progress:  %0%%" />
            <Replacement index="0" val="{$parent/MathFrameB/@ResultRound}" />
