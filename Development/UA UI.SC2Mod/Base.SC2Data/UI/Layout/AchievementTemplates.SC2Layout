            <Anchor relative="$parent" />
            <A val="{$parent/$parent/PointsLabel/CurrentLabel/@Text}" />
            <B val="0" />
            <Operator val="Add" />
         </Frame>
         <Frame type="MathFrame" name="MathFrameB">
            <Anchor relative="$parent" />
            <A val="{$parent/$parent/PointsLabel/MaxLabel/@Text}" />
            <B val="0" />
            <Operator val="Add" />
         </Frame>
         <Frame type="TextFormatFrame" name="FormattedText">
            <Anchor relative="$parent" />
            <FormatText val="%0%/%1%" />
            <Replacement index="0" val="{$parent/MathFrameA/@Result}" />
            <Replacement index="1" val="{$parent/MathFrameB/@Result}" />
         </Frame>
      </Frame>
